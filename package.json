{
  "name": "hard_times",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV='development' webpack-dev-server -c webpack.config.js --silent",
    "build": "yarn _transpile-src && NODE_ENV='production' BABEL_ENV='modern' webpack -c webpack.config.js --silent && yarn _build-inline-template",
    "_build-inline-template": "NODE_ENV='production' ts-node -T --project tsconfig.node.json ./scripts/build.ts",
    "_transpile-src": "NODE_ENV=production node -r esm -r ts-node/register/transpile-only ./scripts/transpile.ts"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "preact": "^10.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@chrstntdd/node": "^0.0.8",
    "@types/babel__core": "^7.1.7",
    "@types/cross-spawn": "^6.0.1",
    "@types/node": "^13.13.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-preval": "^5.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "cross-spawn": "^7.0.2",
    "css-loader": "^3.5.2",
    "extract-css-chunks-webpack-plugin": "^4.7.4",
    "fast-async": "^6.3.8",
    "git-rev-sync": "^2.0.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^4.2.0",
    "linaria": "^1.3.3",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "preact-render-to-string": "^5.1.6",
    "prettier": "^2.0.4",
    "purgecss": "^2.1.2",
    "regenerator-runtime": "^0.13.5",
    "style-loader": "^1.1.4",
    "terser": "^4.6.11",
    "terser-webpack-plugin": "^2.3.5",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-stylish": "^0.1.8"
  },
  "resolutions": {
    "acorn": "^6.4.1",
    "minimist": "^1.2.5"
  },
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "none"
  }
}
